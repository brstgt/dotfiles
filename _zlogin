# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi

PATH="$PATH:$HOME/.composer/vendor/bin:~/.local/bin"
PATH="$PATH:$HOME/repos/backend/backend/vendor/bin"
export PHP_INI_SCAN_DIR=:~/etc/php.d
export GOPATH=~/go
export JAUMO_ENV=Test
export ZPROFILE=1

alias cli=~/repos/backend/backend/cli
eval "$($HOME/.config/composer/vendor/bin/symfony-autocomplete ~/repos/backend/backend/cli)"
